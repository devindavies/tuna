{
	"name": "@devinmdavies/tunajs",
	"version": "1.0.0",
	"description": "Audio effects library for the Web Audio API",
	"main": "./dist/index.js",
	"files": ["dist"],
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"scripts": {
		"prepare": "npm run build",
		"dev": "tsup --watch",
		"build": "tsup",
		"format-and-lint": "biome check .",
		"test": "echo \"Error: no test specified\" && exit 1",
		"release": "changeset version && changeset publish"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Theodeus/tuna.git"
	},
	"keywords": ["web audio", "audio effects", "tuna", "tunajs", "tuna.js"],
	"author": "Oskar Eriksson",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Theodeus/tuna/issues"
	},
	"homepage": "https://github.com/Theodeus/tuna#readme",
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@changesets/cli": "^2.28.1",
		"@types/audioworklet": "^0.0.71",
		"@types/node": "^22.13.10",
		"tsup": "^8.4.0",
		"typescript": "^5.8.2"
	},
	"packageManager": "npm@10.5.0"
}
